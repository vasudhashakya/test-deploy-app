<%= simple_form_for(@vacation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :worker_email %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
